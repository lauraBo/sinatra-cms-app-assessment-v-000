<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>             

 <!DOCTYPE html> 
<html>
  <head>
    <meta charset="UTF-8" />
    <title> User Home Page</title>
  </head>
<body>
  <h1> <%= @user.username%>'s Dogs </h1> </br>
  
  <% @user.dogs.each do |dog| %>
  
 <p> NAME:  <%= dog.name %> </P>
<P> BREED: <%= dog.breed %> </P>
<P>  AGE:<%= dog.age %> </P>
<P> SHELTER: <%= dog.shelter %> </P>
   </br>
   <% end %>


   
 
  <p> <a href='/dogs/new'> Add New Dog </a> </p>  </br>
  <p>  <a href='/dogs'> Public Index </a> </p>  </br>
  <p>   <a href='/logout'> Logout </a> </p>  </br>
   
  

  </body>
</html> 